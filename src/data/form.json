      "label": "Date",
      "has-layout-class": [
        "date"
      ]
    },
    {
      "@id": "rdfs:dates",
      "has_related_question": [
        "rdfs:date",
        "rdfs:time",
        "rdfs:datetime"
      ],
      "has-layout-class": "section",
      "has-media-content": "https://drive.google.com/file/d/18HBeiOb5zthhIFosidc7mRTZtNAd3Ur7/preview",
      "label": "Dates",
      "comment": "This is comment, it is not clever"
    }
