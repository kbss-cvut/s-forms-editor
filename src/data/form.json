  "@graph": [
    {
      "@id": "_:b0",
      "@type": "http://onto.fel.cvut.cz/ontologies/form/condition",
      "accepts-answer-value": true,
      "has-tested-question": "rdfs:checkbox-q"
    },
    {
      "@id": "_:b1",
      "@type": "http://onto.fel.cvut.cz/ontologies/form/condition",
      "accepts-answer-value": true,
      "has-tested-question": "rdfs:checkbox-show-q"
    },
    {
      "@id": "rdfs:checkbox-q",
      "@type": "doc:question",
      "has-layout-class": "checkbox",
      "has-question-origin": "@id",
      "requires-answer": true,
      "label": "Information of the outcome available?"
    },
    {
      "@id": "rdfs:checkbox-show-q",
      "@type": "doc:question",
      "has_related_question": "rdfs:collapsed-q",
      "has-layout-class": [
        "section",
        "checkbox",
        "answerable"
      ],
      "has-question-origin": "@id",
      "label": "Had surgery?"
    },
    {
      "@id": "rdfs:collapsed-q",
      "@type": "doc:question",
      "has-layout-class": [
        "type-ahead"
      ],
      "has-possible-value": [
        {
          "@id": "http://profession.com/chef",
          "label": "Chef"
        },
        {
          "@id": "http://profession.com/developer",
          "label": "Developer"
        }
      ],
      "has-question-origin": "@id",
      "is-relevant-if": "_:b1",
      "label": "Type of surgery"
    },
    {
      "@id": "rdfs:date-q",
      "@type": "doc:date",
      "has-layout-class": [
        "masked-input",
        "collapsed"
      ],
      "has-input-mask": "MM/YYYY",
      "has-preceding-question": "rdfs:label-q",
      "has-question-origin": "@id",
      "requires-answer": true,
      "label": "Date of birth (month/year)"
    },
    {
      "@id": "rdfs:id-q",
      "@type": "doc:question",
      "has-question-origin": "@id",
      "requires-answer": true,
      "label": "First name",
      "has-datatype": "xsd:int",
      "has-unit": "mm",
      "maxInclusive": "20",
      "minInclusive": "0"
    },
    {
      "@id": "rdfs:label-q",
      "@type": "doc:question",
      "has-preceding-question": "rdfs:id-q",
      "has-question-origin": "rdfs:label",
      "requires-answer": true,
      "description": "If middle name, put it here.",
      "label": "Last name"
    },
    {
      "@id": "x:form-1-default-q",
      "@type": "doc:question",
      "has_related_question": [
        "rdfs:checkbox-q",
        "rdfs:friendship",
        "rdfs:dates"
      ],
      "has-layout-class": [
        "section",
        "wizard-step"
      ],
      "label": "Inclusion Criteria"
    },
    {
      "@id": "x:form-1-q",
      "@type": "doc:question",
      "has_related_question": [
        "x:form-1-default-q",
        "x:form-2-default-q",
        "x:form-3-default-q"
      ],
      "has-layout-class": "form"
    },
    {
      "@id": "x:form-2-default-q",
      "@type": "doc:question",
      "has_related_question": [
        "rdfs:id-q",
        "rdfs:label-q",
        "rdfs:date-q"
      ],
      "has-layout-class": [
        "section",
        "wizard-step"
      ],
      "has-preceding-question": "x:form-1-default-q",
      "label": "Personal information"
    },
    {
      "@id": "x:form-3-default-q",
      "@type": "doc:question",
      "has_related_question": "rdfs:checkbox-show-q",
      "has-layout-class": [
        "section",
        "wizard-step"
      ],
      "has-preceding-question": "x:form-2-default-q",
      "is-relevant-if": "_:b0",
      "label": "Treatment"
    },
    {
      "@id": "rdfs:friendship",
      "has_related_question": [
        "rdfs:friend-1",
        "rdfs:friend-2"
      ],
      "has-layout-class": "section",
      "label": "Friendship"
    },
    {
      "@id": "rdfs:friend-1",
      "@type": "doc:question",
      "has-layout-class": [
        "textarea"
      ],
      "has-question-origin": "rdfs:label",
      "label": "Friend 1"
    },
    {
      "@id": "rdfs:friend-2",
      "@type": "doc:question",
      "has-layout-class": "masked-input",
      "has-question-origin": "rdfs:label",
      "label": "Friend 2"
    },
    {
      "@id": "rdfs:datetime",
      "@type": "doc:question",
      "label": "Date and time",
      "has-layout-class": [
        "section",
        "answerable",
        "datetime"
      ]
    },
    {
      "@id": "rdfs:time",
